{
  "name": "zno-library",
  "version": "1.0.0",
  "description": "Web site for preparation to zno",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-client & npm run test-server",
    "build-server": "tsc --p server.tsconfig.json",
    "build-server-tests": "tsc --p server.tests.config.json",
    "build-client": "webpack --display-error-details --cache",
    "test-client": "mocha-phantomjs ./tests/client/runner.html",
    "test-server": "mocha ./tests/server/app/dummy.test.js",
    "clean-static": "RD /S /Q static & MD static",
    "prebuild-production": "npm run clean-static",
    "build-production": "webpack -p",
    "build-all": "npm run build-server & npm run build-server-tests & npm run build-client",
    "watch-client": "webpack --display-error-details --watch",
    "watch-server": "start tsc --p server.tsconfig.json --watch & tsc --p server.tests.config.json --watch",
    "start": "node ./server/app/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitrijbozhkov/zno-library.git"
  },
  "author": "Dmitrij Bozhkov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dmitrijbozhkov/zno-library/issues"
  },
  "homepage": "https://github.com/dmitrijbozhkov/zno-library#readme",
  "dependencies": {
    "@cycle/dom": "^16.0.0",
    "@cycle/history": "^6.1.0",
    "@cycle/http": "^13.2.0",
    "@cycle/run": "^3.1.0",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "marked": "^0.3.6",
    "passport": "^0.3.2",
    "pg": "^6.1.5",
    "pouchdb": "^6.1.2",
    "xstream": "^10.3.0"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/express": "^4.0.35",
    "@types/marked": "0.0.28",
    "@types/mocha": "^2.2.40",
    "@types/mocha-phantomjs": "^3.5.28",
    "@types/node": "^7.0.11",
    "@types/phantomjs": "^1.9.29",
    "@types/pouchdb": "^5.4.28",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "ignore-loader": "^0.1.2",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.1",
    "mocha-phantomjs": "^4.1.0",
    "node-sass": "^4.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "ts-loader": "^2.0.3",
    "tslint": "^4.5.1",
    "typedoc": "^0.5.9",
    "typescript": "^2.2.1",
    "webpack": "^2.3.2"
  }
}
